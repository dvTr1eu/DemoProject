@using Microsoft.AspNetCore.Mvc.TagHelpers
@model             Core.DTOs.SignUpRequest
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">


<!-- Mirrored from pixner.net/boleto/demo/sign-up.html by HTTrack Website Copier/3.x [XR&CO'2014], Thu, 20 Feb 2025 14:43:42 GMT -->
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <link rel="stylesheet" href="~/movieassets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/movieassets/css/all.min.css">
    <link rel="stylesheet" href="~/movieassets/css/animate.css">
    <link rel="stylesheet" href="~/movieassets/css/flaticon.css">
    <link rel="stylesheet" href="~/movieassets/css/magnific-popup.css">
    <link rel="stylesheet" href="~/movieassets/css/odometer.css">
    <link rel="stylesheet" href="~/movieassets/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/movieassets/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/movieassets/css/nice-select.css">
    <link rel="stylesheet" href="~/movieassets/css/main.css">

    <link rel="shortcut icon" href="~/movieassets/images/favicon.png" type="image/x-icon">

    <title>Boleto  - Online Ticket Booking Website HTML Template</title>


</head>

<body>
    <!-- ==========Preloader========== -->
    <div class="preloader">
        <div class="preloader-inner">
            <div class="preloader-icon">
                <span></span>
                <span></span>
            </div>
        </div>
    </div>
    <!-- ==========Preloader========== -->
    <!-- ==========Sign-In-Section========== -->
    <section class="account-section bg_img" data-background="~/movieassets/images/account/account-bg.jpg">
        <div class="container">
            <div class="padding-top padding-bottom">
                <div class="account-area">
                    <div class="section-header-3">
                        <span class="cate">welcome</span>
                        <h2 class="title">to Boleto </h2>
                    </div>
                    <form asp-action="SignUp" asp-controller="Account" method="post" class="account-form">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group">
                            <label for="fullname">FullName<span>*</span></label>
                            <input asp-for="FullName" type="text" placeholder="Enter Your Full Name" id="fullname" required>
                        </div>
                        <div class="form-group">
                            <label for="email">Email<span>*</span></label>
                            <input asp-for="Email" type="email" placeholder="Enter Your Email" id="email" required>
                            <span asp-validation-for="Email" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label for="datebirth">Date Birth<span>*</span></label>
                            <input asp-for="DateBirth" type="date" required>
                        </div>
                        <div class="form-group">
                            <label for="phone">Phone<span>*</span></label>
                            <input asp-for="PhoneNumber" type="text" placeholder="Enter Your Phone" id="phone" required>
                        </div>
                        <div class="form-group">
                            <label for="pass1">Password<span>*</span></label>
                            <input asp-for="Password" type="password" placeholder="Password" id="pass1" required>
                            <span asp-validation-for="Password" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label for="pass2">Confirm Password<span>*</span></label>
                            <input asp-for="ConfirmPassword" type="password" placeholder="Password" id="pass2" required>
                            <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
                        </div>
                        <div class="form-group checkgroup">
                            <input type="checkbox" id="bal" required checked>
                            <label for="bal">I agree to the <a href="#0">Terms, Privacy Policy</a> and <a href="#0">Fees</a></label>
                        </div>
                        <div class="form-group text-center">
                            <input type="submit" value="Sign Up">
                        </div>
                    </form>
                    <div class="option">
                        Already have an account? <a href="/Account/Login">Login</a>
                    </div>
                    <div class="or"><span>Or</span></div>
                    <ul class="social-icons">
                        <li>
                            <a class="active" asp-action="LoginByGoogle" asp-controller="Account">
                                <i class="fab fa-google"></i>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    <!-- ==========Sign-In-Section========== -->


    <script src="~/movieassets/js/jquery-3.3.1.min.js"></script>
    <script src="~/movieassets/js/modernizr-3.6.0.min.js"></script>
    <script src="~/movieassets/js/plugins.js"></script>
    <script src="~/movieassets/js/bootstrap.min.js"></script>
    <script src="~/movieassets/js/isotope.pkgd.min.js"></script>
    <script src="~/movieassets/js/magnific-popup.min.js"></script>
    <script src="~/movieassets/js/owl.carousel.min.js"></script>
    <script src="~/movieassets/js/wow.min.js"></script>
    <script src="~/movieassets/js/countdown.min.js"></script>
    <script src="~/movieassets/js/odometer.min.js"></script>
    <script src="~/movieassets/js/viewport.jquery.js"></script>
    <script src="~/movieassets/js/nice-select.js"></script>
    <script src="~/movieassets/js/main.js"></script>
</body>


